{
    "experiment":{
        "tag": "nnp",
        "use_gpu": true,
        "repeat": 3,
        "seed": null,
        "base_log_dir": "/home/qizhou/data/NeurIPS/alpha"
    },
    "algorithm": {
        "name": "nnp",
        "class": "batch_RL_algorithm",
        "kwargs": {
            "num_epochs": 1000,
            "batch_size": 256,
            "num_eval_steps_per_epoch":8000,
            "num_train_loops_per_epoch": 1000,
            "num_expl_steps_per_train_loop": 1,
            "num_trains_per_train_loop": 1,
            "min_num_steps_before_training": 10000,
            "record_video_freq": 20,
            "silent": false
        }
    },
    "environment": [
        {
            "name": "expl_env",
            "class": "simple_env",
            "kwargs": {
                "env_name": "cheetah",
                "reward_scale": 1
            }
        },
        {
            "name": "eval_env",
            "class": "normalized_vector_env",
            "kwargs": {
                "env_name": "cheetah",
                "n_env":8,
                "reward_scale": 1
            }
        },
        {
            "name": "video_env",
            "class": "video_env",
            "kwargs": {
                "env_name": "cheetah"
            }
        }
    ],
    "policy": [
        {
            "name": "init_expl_policy",
            "class": "uniformly_random_policy",
            "kwargs":{
                "env": "$expl_env"
            }
        },
        {
            "name": "policy",
            "class": "multi_head_policy",
            "kwargs": {
                "env": "$expl_env",
                "hidden_layers": [256,256],
                "nonlinearity": "relu"
            }
        },
        {
            "name": "eval_policy",
            "class": "MakeDeterministic",
            "kwargs":{
                "random_policy": "$policy"
            }
        }
    ],
    "value": {
        "name": "qf",
        "class": "ensemble_q_value",
        "kwargs": {
            "env": "$expl_env",
            "hidden_layers": [256,256]
        } 
    },
    "trainer":{
        "name": "trainer",
        "class": "NNP_trainer_MH",
        "kwargs": {
            "env": "$expl_env",
            "policy": "$policy",
            "qf": "$qf",
            "reward_scale":1,
            "policy_lr": 3e-4,
            "qf_lr": 3e-4,
            "soft_target_tau": 5e-3,
            "use_automatic_bonus_tuning": true,
            "alpha_if_not_automatic": 0,
            "bonus_type": "phi_power",
            "exponent": 0.25,
            "target_gaussian_std": 0.20
        }
    },
    "pool": {
        "name": "pool",
        "class": "simple_pool",
        "kwargs": {
            "env": "$expl_env",
            "max_size": 1e6
        }
    },
    "collector": [
        {
            "name": "expl_collector",
            "class": "simple_step_collector",
            "kwargs": {
                "env": "$expl_env",
                "policy": "$policy"
            }
        },
        {
            "name": "eval_collector",
            "class": "simple_path_collector",
            "kwargs": {
                "env": "$eval_env",
                "policy": "$eval_policy"
            }
        }
    ]
}