{
    "base_config_file": "configs/surprise-based/ant_maze_resource/surprise_vision/surprise_vision.json",
    "experiment":{
        "tag": "surprise_vision",
        "use_gpu": true,
        "seed": 10,
        "base_log_dir": "/home/rl_shared/zhihaiwang/research/ICML_data/exploration_env_exps_fix_env/ant_corridor/valid_experiments/with_entropy_int005/goal5/our_methods/sac_surprise_vision",
        "exp_prefix": "ant_corridor_surprise_vision_goal5_with_entropy_int005"
    },
    "algorithm": {
        "name": "model_based_sac_surprise",
        "class": "ModelBasedBatchRLAlgorithm",
        "kwargs": {
            "num_epochs": 3000,
            "batch_size": 256,
            "num_eval_steps_per_epoch":8000,
            "num_train_loops_per_epoch": 1000,
            "num_expl_steps_per_train_loop": 1,
            "num_trains_per_train_loop": 1,
            "num_train_models_per_epoch": 1000,
            "max_path_length": 500,
            "train_model_freq": 25,
            "min_num_steps_before_training": 5000,
            "record_video_freq": 10,
            "silent": false
        }
    },
    "trainer":{
        "name": "trainer",
        "class": "Vision_Surprise_SAC_Trainer",
        "kwargs": {
            "env": "$expl_env",
            "policy": "$policy",
            "qf": "$qf",
            "model": "$model_without_reward",
            "intrinsic_coeff": 0.05,
            "model_lr": 3e-4,
            "training_noise_stdev": 0.1,
            "grad_clip": 1,
            "reward_scale":1,
            "policy_lr": 3e-4,
            "qf_lr": 3e-4,
            "soft_target_tau": 5e-3,
            "use_automatic_entropy_tuning": true,
            "alpha_if_not_automatic": 0,
            "discount": 0.99
        }
    },
    "environment": [
        {
            "name": "expl_env",
            "class": "simple_env",
            "kwargs": {
                "env_name": "ant_corridor_resource_env_goal_5_v0",
                "reward_scale": 1
            }
        },
        {
            "name": "eval_env",
            "class": "normalized_vector_env",
            "kwargs": {
                "env_name": "ant_corridor_resource_env_goal_5_v0",
                "n_env":8,
                "reward_scale": 1
            }
        },
        {
            "name": "video_env",
            "class": "video_env",
            "kwargs": {
                "env_name": "ant_corridor_resource_env_goal_5_v0"
            }
        }
    ]
}