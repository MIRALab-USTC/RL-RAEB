{
    "base_config_file": "configs/aceb/aceb.json",
    "trainer":{
        "name": "trainer",
        "class": "ACEB_trainer_MH",
        "kwargs": {
            "env": "$expl_env",
            "policy": "$policy",
            "qf": "$qf",
            "reward_scale":1,
            "policy_lr": 3e-4,
            "qf_lr": 3e-4,
            "soft_target_tau": 5e-3,
            "use_automatic_bonus_tuning": true,
            "alpha_if_not_automatic": 0,
            "bonus_type": "phi_power",
            "exponent": 0.25,
            "target_gaussian_std": 0.09
        }
    }
}